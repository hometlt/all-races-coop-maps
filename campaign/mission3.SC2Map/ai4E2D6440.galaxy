    ai4E2D6440_waves_trigger = TriggerCreate("ai4E2D6440_waves_runFunc");
}

//--------------------------------------------------------------------------------------------------
bool ai4E2D6440_waves_runFunc (bool c, bool a) {
    int p = ai4E2D6440_waves_runPlayer;
    playergroup target = ai4E2D6440_waves_runTarget;

    if (p < 0 || p >= 16) {
        return false;
    }

    ai4E2D6440_wave1_run(p, target, false);
    Wait(0, c_timeAI);
    ai4E2D6440_wave2_run(p, target, true);
    ai4E2D6440_wave5_run(p, target, true);
    ai4E2D6440_wave3_run(p, target, true);
    ai4E2D6440_wave6_run(p, target, true);
    ai4E2D6440_wave4_run(p, target, true);
    ai4E2D6440_wave11_run(p, target, true);
    ai4E2D6440_wave8_run(p, target, true);
    ai4E2D6440_wave7_run(p, target, true);
    ai4E2D6440_wave9_run(p, target, true);
    ai4E2D6440_wave10_run(p, target, true);

    while (true) {
        ai4E2D6440_wave9_run(p, target, true);
        ai4E2D6440_wave10_run(p, target, true);
    }

    return true;
}

//--------------------------------------------------------------------------------------------------
void ai4E2D6440_waves_run (int p, playergroup target) {
    ai4E2D6440_waves_runPlayer = p;
    ai4E2D6440_waves_runTarget = PlayerGroupCopy(target);

    TriggerExecute(ai4E2D6440_waves_trigger, false, false);
}

//--------------------------------------------------------------------------------------------------
void ai4E2D6440_waves_enable (int p, bool val) {
    if (p < 0 || p >= 16) {
        return;
    }

    ai4E2D6440_wave1_enable(p, val);
    ai4E2D6440_wave2_enable(p, val);
    ai4E2D6440_wave5_enable(p, val);
    ai4E2D6440_wave3_enable(p, val);
    ai4E2D6440_wave6_enable(p, val);
    ai4E2D6440_wave4_enable(p, val);
    ai4E2D6440_wave11_enable(p, val);
    ai4E2D6440_wave8_enable(p, val);
    ai4E2D6440_wave7_enable(p, val);
    ai4E2D6440_wave9_enable(p, val);
    ai4E2D6440_wave10_enable(p, val);
}

//--------------------------------------------------------------------------------------------------
void ai4E2D6440_waves_stop () {
    TriggerStop(ai4E2D6440_waves_trigger);

    TriggerStop(ai4E2D6440_wave1_trigger);
    TriggerStop(ai4E2D6440_wave2_trigger);
    TriggerStop(ai4E2D6440_wave5_trigger);
    TriggerStop(ai4E2D6440_wave3_trigger);
    TriggerStop(ai4E2D6440_wave6_trigger);
    TriggerStop(ai4E2D6440_wave4_trigger);
    TriggerStop(ai4E2D6440_wave11_trigger);
    TriggerStop(ai4E2D6440_wave8_trigger);
    TriggerStop(ai4E2D6440_wave7_trigger);
    TriggerStop(ai4E2D6440_wave9_trigger);
    TriggerStop(ai4E2D6440_wave10_trigger);
}

