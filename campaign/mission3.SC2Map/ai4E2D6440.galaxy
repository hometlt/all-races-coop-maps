//--------------------------------------------------------------------------------------------------
// Start
//--------------------------------------------------------------------------------------------------
bool ai4E2D6440_initialized = false;

//--------------------------------------------------------------------------------------------------
void ai4E2D6440_start (int p) {
    int init_i;

    if (p < 0 || p >= 16) {
        return;
    }
    if (!ai4E2D6440_initialized) {
        ai4E2D6440_wave1_init();
        ai4E2D6440_wave2_init();
        ai4E2D6440_wave5_init();
        ai4E2D6440_wave3_init();
        ai4E2D6440_wave6_init();
        ai4E2D6440_wave4_init();
        ai4E2D6440_wave11_init();
        ai4E2D6440_wave8_init();
        ai4E2D6440_wave7_init();
        ai4E2D6440_wave9_init();
        ai4E2D6440_wave10_init();
        ai4E2D6440_waves_init();

        for (init_i = 0; init_i <= 16; init_i += 1) {
            ai4E2D6440_defGather[init_i] = PointFromId(1158110219);
        }

        ai4E2D6440_initialized = true;
    }

    AICampaignStart(p);
}
