bool gt_Victory_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    // Conditions
    if (testConds) {
        if (!((UnitIsAlive(UnitFromId(53904036)) != true))) {
            return false;
        }

        if (!((UnitIsAlive(UnitFromId(1550270736)) != true))) {
            return false;
        }

        if (!((UnitIsAlive(UnitFromId(271831681)) != true))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    gv_enviromenteffects = false;
    gv_gameended = true;
    UIClearMessages(PlayerGroupAll(), c_messageAreaAll);
    Wait(0.1, c_timeGame);
    ObjectiveSetState(DataTableGetObjective(true, "1"), c_objectiveStateCompleted);
