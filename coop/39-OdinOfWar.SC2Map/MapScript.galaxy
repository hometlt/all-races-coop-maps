    libCOMI_gf_CinematicModeCampaign(true, c_transitionDurationDefault, true);
    CameraApplyInfo(1, CameraInfoFromId(1923027717), 1.0, -1, 10.0, true);
    CameraApplyInfo(2, CameraInfoFromId(1923027717), 1.0, 100.0, 10.0, true);
