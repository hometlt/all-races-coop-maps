// Trigger: Defeat Odin Timer
//--------------------------------------------------------------------------------------------------
bool gt_DefeatOdinTimer_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    // Conditions
    if (testConds) {
        if (!((TriggerIsEnabled(TriggerGetCurrent()) == true))) {
            return false;
        }

        if (!((libCOMI_gf_GameIsOver() == false))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    UIClearMessages(PlayerGroupAll(), c_messageAreaAll);
    libCOMI_gf_GameOver(libCOMI_ge_CampaignMissionGameResultType_Defeat);
    TransmissionClearAll();
    TriggerQueuePause(true);
    TriggerQueueClear(c_triggerQueueRemove);
    libCOMI_gf_SendMissionTransmission("DestroyedReminder", false, -1);
    libCOMI_gf_CinematicModeCampaign(true, 1.5, true);
    Wait(1.0, c_timeGame);
    CameraLockInput(1, true);
    CameraLockInput(2, true);
    UnitClearSelection(1);
    UnitClearSelection(2);
    UISetSelectionTypeEnabled(PlayerGroupAll(), c_localSelectionTypeUnknown, false);
    CameraPan(1, PointFromId(1945616116), 1.5, -1, 10.0, false);
    CameraPan(2, PointFromId(1945616116), 1.5, -1, 10.0, false);
    libCOMI_gf_DisplayCampaignMessage(libCOMI_gf_HumanPlayers(), libCOMI_ge_CampaignMessageType_MissionFailed, StringExternal("Param/Value/F50A9013"), libCOMI_ge_CampaignMessageFormatNotFormat_FormatMessage);
    Wait(5.0, c_timeGame);
    libCOMI_gf_EndCampaignMission(c_gameOverDefeat);
    return true;
}

