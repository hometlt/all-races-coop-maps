        if ((gv_keystoneChargeCurrent >= (gv_keystoneChargeMax * 0.25)) && (TriggerIsEnabled(gt_KeystoneQuarterSave) == true)) {
            TriggerExecute(gt_KeystoneQuarterSave, true, false);
        }

    }
    return true;
}

