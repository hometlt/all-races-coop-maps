            libNtve_gf_SendActorMessageToUnit(gv_casterKeystone, "Signal PlaySoundB");
        }

        if ((gv_keystoneChargeCurrent >= (gv_keystoneChargeMax * 0.5)) && (TriggerIsEnabled(gt_KeystoneHalfQ) == true)) {
